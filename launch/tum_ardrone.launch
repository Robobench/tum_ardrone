<launch>
  <arg name="debug" default="false"/>
  <arg name="gui" default="true" />

  <node name="drone_stateestimation" pkg="tum_ardrone" type="drone_stateestimation" launch-prefix="gdb -ex run --args" if="$(arg debug)">
  </node>
  <node name="drone_stateestimation" pkg="tum_ardrone" type="drone_stateestimation" unless="$(arg debug)">
  </node>

  <node name="drone_autopilot" pkg="tum_ardrone" type="drone_autopilot">
  </node>

  <node name="drone_gui" pkg="tum_ardrone" type="drone_gui" if="$(arg gui)">
  </node>
</launch>
